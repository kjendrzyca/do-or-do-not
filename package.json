{
  "name": "do-or-do-not",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "basic-auth": "^1.1.0",
    "body-parser": "^1.17.2",
    "chance": "^1.0.8",
    "cuid": "^1.3.8",
    "express": "^4.15.3",
    "mongoose": "^4.10.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.3"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "deep-freeze": "^0.0.1",
    "enzyme": "^2.8.2",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.5.1",
    "react-scripts": "1.0.1"
  },
  "scripts": {
    "start": "concurrently \"yarn run server:watch\" \"yarn run client\"",
    "client": "REACT_APP_PASS=foo:bar react-scripts start",
    "server": "PASS=foo:bar node src/server.js",
    "server:watch": "PASS=foo:bar nodemon src/server.js",
    "build": "REACT_APP_PASS=foo:bar react-scripts build",
    "test": "react-scripts test --env=jsdom --verbose",
    "eject": "react-scripts eject",
    "deploy": "now",
    "now-start": "NODE_ENV=production npm run server"
  },
  "proxy": "http://localhost:3001/"
}
